        <% @places.zip(@image_urls).each do |place, image_url| %>
        <%= link_to place.name, place_path(place) %><br>
        <%=image_tag("#{place.image_url}", class: 'yelp-icon')%>
        <%= place.address %>
        <%= place.city %>, <%= place.state_code %> <%= place.postal_code%>
        <% if place.neighborhoods %>
        Neighborhood: <%= place.neighborhoods %>
        <% end %>
        <%= link_to 'Map it!', 'http://www.google.com/maps/place/#{place.coordinate_latitude},#{place.coordinate_longitude}', :target => "_blank" %><br>
        <% end %>