<%= render partial: 'layouts/form' %>

<dl class="intro-views">
    <% @profile.businesses.each do |biz| %>
    
    <dd>
    <h1><%= link_to biz.name, places_path %></a></h1><br>
    <img src="<%= biz.image_url %>"><br>
    <%= biz.location.address.join %><br>
    <%= biz.location.city %>, <%= biz.location.state_code %> <%= biz.location.postal_code %> <br>
    Neighborhood: <%= biz.location.neighborhoods.join %><br>
    <%= biz.location.coordinate.latitude %>
    <%= biz.location.coordinate.longitude %><br>
    Phone:<%= biz.display_phone %><br>
    <hr>
  <% end %>
  
</dl>
