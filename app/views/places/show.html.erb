  <p><h2><strong><%= @place.name %>:</strong></h2></p>
  <p><strong>Address:</strong><%= @place.address %> <%= @place.city %> <%= @place.state_code %> <%= @place.postal_code %><br>
  <strong>Neighborhood: </strong><%= @place.neighborhoods %><br>
  <strong>Phone:</strong><%= @place.phone %></p>
  <br>
  <%= render partial: 'layouts/iframe' %>
  <div class="row">
    <div class="small-2 large-2 columns">
      <%= imdb_style_rating_for @place, current_user %>
    </div>
    <br>
    <div class="small-2 large-4 columns">
      <% if current_user %>
        <ul>
          <li id="atmos-rating">Atmosphere:<br>  
            <%= rating_for @place, "atmosphere", imdb_avg: true %>
          </li>
          <br>
          <li id="drink-rating">Drinks:<br>  
            <%= rating_for @place, "drinks", cancel_hint: 'Cancel this vote!', imdb_avg: true, space: true %>
          </li>
          <br>
          <li id="wait-rating">Wait Time:<br> 
            <%= rating_for @place, "wait_time", cancel_hint: 'Cancel this vote!', imdb_avg: true, space: true %>
          </li>
          <br>
          <li id="cost-rating">Cost: <br> 
            <%= rating_for @place, "cost", cancel_hint: 'Cancel this vote!', imdb_avg: true, space: true %>
          </li>
          <br>
          <li id="music-rating">Music:<br> 
            <%= rating_for @place, "music", cancel_hint: 'Cancel this vote!', imdb_avg: true, space: true %>
          </li>
          <br>
          <%= button_tag "Submit", onclick: "savePlaceRating()" %>
      <% end %>
    </div>
  </div>
  <br>
  <br>
  <br>
